{
  "author": "planttheidea <tony.quetano@planttheidea.com>",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "files": [
      "./test/utils.js",
      "./test/index.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/selectorator/issues"
  },
  "dependencies": {
    "fast-equals": "^1.2.1",
    "identitate": "^1.0.0",
    "reselect": "^3.0.1",
    "unchanged": "^1.0.7"
  },
  "description": "Simplified generator of reselect selectors",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "benchmark": "^2.1.4",
    "cli-table2": "^0.2.0",
    "eslint": "^4.18.2",
    "eslint-config-rapid7": "^2.11.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "html-webpack-plugin": "^2.30.1",
    "in-publish": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.4.0",
    "jsdom": "^11.6.2",
    "moize": "^4.0.4",
    "nyc": "^11.4.1",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "homepage": "https://github.com/planttheidea/selectorator#readme",
  "keywords": [
    "reselect",
    "selector"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "name": "selectorator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/selectorator.git"
  },
  "scripts": {
    "benchmark": "rimraf lib && npm run transpile:lib && node ./benchmark/index.js",
    "build": "NODE_ENV=development webpack --progress --colors --config=webpack/webpack.config.js",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack/webpack.config.minified.js",
    "clean": "rimraf lib && rimraf es && rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --progress --colors --config=webpack/webpack.config.dev.js",
    "docs": "rimraf docs && jsdoc -c ./jsdoc.config.json && git add --all",
    "lint": "eslint --max-warnings 0 src",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run clean && npm run transpile:lib && npm run transpile:es && npm run build && npm run build:minified && npm run docs",
    "start": "npm run dev",
    "test": "NODE_ENV=test ava",
    "test:coverage": "nyc --cache npm test",
    "test:watch": "npm run test -- --watch",
    "transpile:es": "BABEL_ENV=es babel src --out-dir es",
    "transpile:lib": "BABEL_ENV=lib babel src --out-dir lib"
  },
  "version": "3.3.0"
}
